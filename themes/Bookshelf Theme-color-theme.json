    "tokenColors": [
        {
            "name": "Comments",
            "scope": "comment",
            "settings": {
                "foreground": "#006e09ee",
				"fontStyle": "italic"
            }
        },
        {
            "name": "Constants",
            "scope": "constant",
            "settings": {
                "foreground": "#990F3D",
            }
        },
        {
            "name": "Escape characters",
            "scope": "constant.character.escape",
            "settings": {
                "foreground": "#0D7680"
            }
        },
        {
            "name": "Named entities",
            "scope": "entity.name",
            "settings": {
                "foreground": "#a73c0b"
            }
        },
        {
            "name": "Function names",
            "scope": "entity.name.function",
            "settings": {
                "foreground": "#0D7680"
            }
        },
        {
            "name": "Tags",
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#0F5499",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Static types",
            "scope": [
                "entity.name.type",
                "storage.type.cs"
            ],
            "settings": {
                "foreground": "#c24919"
            }
        },
        {
            "name": "Tag attributes",
            "scope": "entity.other.attribute-name",
            "settings": {
                "foreground": "#990F3D",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Inherited classes",
            "scope": "entity.other.inherited-class",
            "settings": {
                "foreground": "#DC3318"
            }
        },
        {
            "name": "CSS ID selectors",
            "scope": "entity.other.attribute-name.id",
            "settings": {
                "foreground": "#0D7680"
            }
        },
        {
            "name": "Pseudo CSS",
            "scope": [
                "entity.other.attribute-name.pseudo-element",
                "entity.other.attribute-name.pseudo-class"
            ],
            "settings": {
                "foreground": "#DC3318"
            }
        },
        {
            "name": "Variable names",
            "scope": [
                "entity.name.variable",
                "variable"
            ],
            "settings": {
                "foreground": "#0F5499"
            }
        },
        {
            "name": "Keywords",
            "scope": "keyword",
            "settings": {
                "foreground": "#800d33",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Operators",
            "scope": "keyword.operator",
            "settings": {
                "foreground": "#333333"
            }
        },
        {
            "name": "Special operators",
            "scope": [
                "keyword.operator.new",
                "keyword.operator.expression",
                "keyword.operator.logical",
                "keyword.operator.delete"
            ],
            "settings": {
                "foreground": "#CC0000"
            }
        },
        {
            "name": "Units",
            "scope": "keyword.other.unit",
            "settings": {
                "foreground": "#990F3D"
            }
        },
        {
            "name": "Markup quotes",
            "scope": "markup.quote",
            "settings": {
                "foreground": "#DC3318B3",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markup headings",
            "scope": [
                "markup.heading",
                "entity.name.section"
            ],
            "settings": {
                "foreground": "#0F5499"
            }
        },
        {
            "name": "Markup bold",
            "scope": "markup.bold",
            "settings": {
                "foreground": "#800d33",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markup italics",
            "scope": "markup.italic",
            "settings": {
                "foreground": "#0D7680",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markup code",
            "scope": [
                "markup.inline.raw",
                "markup.fenced_code.block"
            ],
            "settings": {
                "foreground": "#990F3D"
            }
        },
        {
            "name": "Markup links",
            "scope": "markup.underline.link",
            "settings": {
                "foreground": "#DC3318"
            }
        },
        {
            "name": "Storage keywords",
            "scope": "storage",
            "settings": {
                "foreground": "#593380",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string.quoted",
                "string.template"
            ],
            "settings": {
                "foreground": "#DC3318"
            }
        },
        {
            "name": "Regex",
            "scope": "string.regexp",
            "settings": {
                "foreground": "#990F3D"
            }
        },
        {
            "name": "Markup link descriptions",
            "scope": "string.other.link",
            "settings": {
                "foreground": "#990F3D"
            }
        },
        {
            "name": "Provided (support) values",
            "scope": "support",
            "settings": {
                "foreground": "#f76126"
            }
        },
        {
            "name": "Provided functions",
            "scope": "support.function",
            "settings": {
                "foreground": "#0D7680"
            }
        },
        {
            "name": "Provided variables",
            "scope": "support.variable",
            "settings": {
				"foreground": "#FF8833",
				"fontStyle": "bold"
            }
        },
        {
            "name": "Object/JSON property names",
            "scope": [
                "support.type.property-name",
                "meta.object-literal.key"
            ],
            "settings": {
                "foreground": "#0F5499"
            }
        },
        {
            "name": "CSS property names",
            "scope": "support.type.property-name.css",
            "settings": {
                "foreground": "#333333"
            }
        },
        {
            "name": "Language variables",
            "scope": [
                "variable.language"
            ],
            "settings": {
                "foreground": "#c24919",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Parameters",
            "scope": "variable.parameter",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "Embedded template punctuation",
            "scope": "string.template meta.embedded",
            "settings": {
                "foreground": "#333333"
            }
        },
        {
            "name": "Tag brackets",
            "scope": "punctuation.definition.tag",
            "settings": {
                "foreground": "#0F5499B3",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Key/value separators",
            "scope": "punctuation.separator",
            "settings": {
                "foreground": "#333333"
            }
        },
        {
            "name": "Template expressions",
            "scope": "punctuation.definition.template-expression",
            "settings": {
                "foreground": "#800d33"
            }
        },
        {
            "name": "Embedded section punctuation",
            "scope": "punctuation.section.embedded",
            "settings": {
				"foreground": "#593380",
				"fontStyle": "bold"
            }
        },
        {
            "name": "Markup list punctuation",
            "scope": "punctuation.definition.list",
            "settings": {
                "foreground": "#990F3D"
            }
        }
    ]
}